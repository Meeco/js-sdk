{
  "name": "@meeco/style-kit",
  "version": "0.0.2",
  "description": "Meeco branded components and style mixins",
  "main": "dist/index.css",
  "scripts": {
    "build": "webpack",
    "demo": "webpack-dev-server demo",
    "cypress": "cypress open",
    "docs": "build-storybook -c .storybook -o ../../docs/style-kit",
    "start": "npm run storybook",
    "storybook": "start-storybook -s . -p 8085 --ci",
    "test": "start-server-and-test 8085 'cypress run'",
    "update-snapshots": "start-server-and-test 8085 'cypress run --env updateSnapshots=true'",
    "posttest": "npm run format:check",
    "format:check": "prettier -c \"{stories,cypress,src}/**/*.{ts,html,scss,js}\"",
    "format:write": "prettier --write \"{stories,cypress,src}/**/*.{ts,html,scss,js}\" *.{json,scss} README.md"
  },
  "author": "Meeco",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@cypress/skip-test": "^2.5.0",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-docs": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-storysource": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/html": "^5.3.19",
    "@storybook/preset-scss": "^1.0.2",
    "@storybook/theming": "^5.3.19",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "color": "^3.1.2",
    "cypress": "^4.9.0",
    "cypress-image-snapshot": "^3.1.1",
    "cypress-storybook": "^0.2.0",
    "js-base64": "2.5.1",
    "mini-css-extract-plugin": "^0.9.0",
    "progress-bar-webpack-plugin": "^2.1.0",
    "sass-loader": "^9.0.2",
    "start-server-and-test": "^1.11.0",
    "style-loader": "^1.2.1",
    "webpack": "^4.44.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
