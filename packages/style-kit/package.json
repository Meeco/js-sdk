{
  "name": "@meeco/style-kit",
  "version": "2.0.1",
  "description": "Meeco branded components and style mixins",
  "style": "build/main.css",
  "main": "build/bundle.js",
  "scripts": {
    "build": "webpack",
    "cypress": "cypress open",
    "docs": "build-storybook -c .storybook -o ../../docs/style-kit",
    "start": "npm run storybook",
    "storybook": "start-storybook -s . -p 8085 --ci",
    "test": "start-server-and-test 8085 'cypress run'",
    "update-snapshots": "start-server-and-test 8085 'cypress run --env updateSnapshots=true'",
    "posttest": "npm run format:check",
    "format:check": "prettier -c \"{stories,cypress,src}/**/*.{ts,html,scss,js}\"",
    "format:write": "prettier --write \"{stories,cypress,src}/**/*.{ts,html,scss,js}\" *.{json,scss} README.md"
  },
  "author": "Meeco",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@cypress/skip-test": "^2.6.0",
    "@mdx-js/react": "^2.1.2",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-controls": "^6.5.9",
    "@storybook/addon-docs": "^6.5.9",
    "@storybook/addon-storysource": "^6.5.9",
    "@storybook/addons": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/html": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/theming": "^6.5.9",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "color": "^3.1.3",
    "css-loader": "^6.7.1",
    "cypress-image-snapshot": "^4.0.1",
    "cypress-storybook": "^0.5.1",
    "cypress": "^13.6.5",
    "ignore-loader": "^0.1.2",
    "js-base64": "2.6.4",
    "mini-css-extract-plugin": "^2.6.1",
    "progress-bar-webpack-plugin": "^2.1.0",
    "sass-loader": "^13.0.2",
    "sass": "^1.53.0",
    "start-server-and-test": "^1.12.3",
    "style-loader": "^3.3.1",
    "webpack-cli": "^4.10.0",
    "webpack": "^5.73.0"
  }
}
