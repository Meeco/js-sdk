{
  "name": "@meeco/sdk",
  "description": "A wrapper library around Meeco's various API SDK's and encryption libraries",
  "version": "7.0.0",
  "author": "Meeco",
  "bugs": "https://github.com/Meeco/cli/issues",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/src/index.d.ts",
  "dependencies": {
    "@meeco/cryppo": "2.0.2",
    "@meeco/identity-network-api-sdk": "1.1.0-develop.20230808164050.d37ca3c",
    "@meeco/keystore-api-sdk": "10.1.1-develop.20230816154849.c7c2018",
    "@meeco/vault-api-sdk": "39.6.1-develop.20231213134427.3dde2fcb3",
    "@meeco/vc-api-sdk": "10.0.0-develop.20241004122546.1da229b",
    "@noble/curves": "^1.4.0",
    "@noble/hashes": "^1.4.0",
    "bs58": "5.0.0",
    "buffer": "^6.0.3",
    "chalk": "^4.1.2",
    "debug": "^4.3.4",
    "did-jwt": "^8.0.4",
    "form-data": "^4.0.0",
    "jsrp": "~0.2.4",
    "multiformats": "^9.9.0",
    "parameterize-js": "^1.0.0",
    "tslib": "^2.6.3",
    "uuid": "^9.0.1",
    "varint": "^6.0.0"
  },
  "devDependencies": {
    "@oclif/config": "^1.18.3",
    "@oclif/test": "^2.1.0",
    "@types/chai": "^4.3.1",
    "@types/debug": "^4.1.7",
    "@types/jsrp": "~0.2.3",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.19.43",
    "@types/node-forge": "~0.9.7",
    "@types/sinon": "^10.0.12",
    "chai": "^4.3.6",
    "prettier": "^2.7.1",
    "sinon": "^14.0.0",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tsup": "^8.2.4",
    "typedoc": "^0.23.2",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=10.0.0"
  },
  "files": [
    "/dist"
  ],
  "homepage": "https://github.com/Meeco/cli",
  "keywords": [
    "meeco",
    "sdk",
    "meeco sdk"
  ],
  "license": "MIT",
  "repository": "Meeco/cli",
  "scripts": {
    "build": "tsc -p tsconfig.build.json && tsup src/index.ts --format esm,cjs",
    "format:check": "prettier -c \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:write": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" *.json README.md",
    "lint": "tslint -p . -t stylish -c ../../tslint.json && tslint -p test/tsconfig.json -t stylish -c ../../tslint.json",
    "posttest": "npm run lint && npm run format:check",
    "prepack": "npm run build",
    "build:typedoc": "typedoc --out ../../docs/js-sdk --tsconfig tsconfig.json --cleanOutputDir --name 'Meeco SDK Documentation' --entryPoints AuthData  --excludePrivate src/index.ts",
    "docs": "npm run build:typedoc",
    "test": "ts-mocha -p test/tsconfig.json \"test/**/*.test.ts\""
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "overrides": {
    "@oclif/test": {
      "fancy-test": {
        "nock": "14.0.0-beta.9"
      }
    }
  }
}
