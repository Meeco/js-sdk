{
  "name": "@meeco/sdk",
  "description": "A wrapper library around Meeco's various API SDK's and encryption libraries",
  "version": "0.0.2",
  "author": "Meeco",
  "bugs": "https://github.com/Meeco/cli/issues",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/src/index.d.ts",
  "dependencies": {
    "@meeco/cryppo": "^0.5.0",
    "@meeco/keystore-api-sdk": "0.13.0",
    "@meeco/vault-api-sdk": "^0.14.0",
    "@types/node-forge": "~0.9.0",
    "base-x": "~3.0.7",
    "chalk": "^4.0.0",
    "debug": "^4.1.1",
    "form-data": "^3.0.0",
    "jimp": "^0.12.1",
    "jsrp": "~0.2.4",
    "node-fetch": "^2.6.0",
    "node-forge": "0.8.5",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@oclif/test": "^1.2.6",
    "@types/base-x": "^3.0.6",
    "@types/chai": "~4.2.7",
    "@types/debug": "^4.1.5",
    "@types/express": "~4.17.2",
    "@types/jsrp": "~0.2.1",
    "@types/mocha": "~5.2.7",
    "@types/node": "^10.14.18",
    "@types/sinon": "~7.5.1",
    "@types/yaml": "~1.2.0",
    "bili": "^4.10.0",
    "chai": "~4.2.0",
    "lerna": "^3.22.0",
    "mocha": "~7.0.0",
    "nock": "~11.7.2",
    "nyc": "~15.0.0",
    "prettier": "~1.19.1",
    "rollup-plugin-typescript2": "^0.27.1",
    "sinon": "~8.1.1",
    "ts-mock-imports": "^1.3.0",
    "ts-node": "^8.1.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.16.0",
    "typescript": "^3.9.3"
  },
  "engines": {
    "node": ">=12.4.0"
  },
  "files": [
    "/dist"
  ],
  "homepage": "https://github.com/Meeco/cli",
  "keywords": [
    "meeco",
    "sdk",
    "meeco sdk"
  ],
  "license": "MIT",
  "repository": "Meeco/cli",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.build.json && bili src/index.ts --format cjs --format es --module-name meeco-sdk",
    "format:check": "prettier -c \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:write": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" *.json README.md",
    "lint": "tslint -p . -t stylish -c ../../tslint.json && tslint -p test/tsconfig.json -t stylish -c ../../tslint.json",
    "posttest": "npm run lint && npm run format:check",
    "prepack": "npm run build",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\""
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
